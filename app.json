{
  "expo": {
    "name": "Raxus",
    "slug": "Raxus",
    "version": "0.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "test",
      "buildNumber": "1",
      "deploymentTarget": "13.4",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes for device setup.",
        "NSLocationWhenInUseUsageDescription": "This app uses location services to detect your local network for smart home device configuration.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses location services to detect your local network for smart home device configuration.",
        "NSLocalNetworkUsageDescription": "This app needs access to your local network to communicate with smart home devices.",
        "NSMicrophoneUsageDescription": "This app may use the microphone for voice control features.",
        "UIBackgroundModes": [
          "background-processing"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "react-native-permissions",
        {
          "iosPermissions": [
            "CAMERA",
            "LOCATION_WHEN_IN_USE",
            "LOCATION_ALWAYS",
            "LOCAL_NETWORK"
          ],
          "androidPermissions": [
            "android.permission.CAMERA",
            "android.permission.ACCESS_FINE_LOCATION",
            "android.permission.ACCESS_COARSE_LOCATION",
            "android.permission.ACCESS_WIFI_STATE",
            "android.permission.INTERNET",
            "android.permission.RECORD_AUDIO",
            "android.permission.VIBRATE"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.htoo_khant002.boltexponativewind"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "beb609cd-dc65-4872-9880-35bddf44b29b"
      }
    }
  }
}